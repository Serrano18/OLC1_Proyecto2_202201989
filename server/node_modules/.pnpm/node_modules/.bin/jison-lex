#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/evams/Documents/OLC1_Proyecto2_202201989/server/node_modules/.pnpm/jison-lex@0.3.4/node_modules/jison-lex/node_modules:/mnt/c/Users/evams/Documents/OLC1_Proyecto2_202201989/server/node_modules/.pnpm/jison-lex@0.3.4/node_modules:/mnt/c/Users/evams/Documents/OLC1_Proyecto2_202201989/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/evams/Documents/OLC1_Proyecto2_202201989/server/node_modules/.pnpm/jison-lex@0.3.4/node_modules/jison-lex/node_modules:/mnt/c/Users/evams/Documents/OLC1_Proyecto2_202201989/server/node_modules/.pnpm/jison-lex@0.3.4/node_modules:/mnt/c/Users/evams/Documents/OLC1_Proyecto2_202201989/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jison-lex/cli.js" "$@"
else
  exec node  "$basedir/../jison-lex/cli.js" "$@"
fi
