#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/evams/Documents/OLC1_Proyecto2_202201989/server/node_modules/.pnpm/jison@0.4.18/node_modules/jison/lib/node_modules:/mnt/c/Users/evams/Documents/OLC1_Proyecto2_202201989/server/node_modules/.pnpm/jison@0.4.18/node_modules/jison/node_modules:/mnt/c/Users/evams/Documents/OLC1_Proyecto2_202201989/server/node_modules/.pnpm/jison@0.4.18/node_modules:/mnt/c/Users/evams/Documents/OLC1_Proyecto2_202201989/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/evams/Documents/OLC1_Proyecto2_202201989/server/node_modules/.pnpm/jison@0.4.18/node_modules/jison/lib/node_modules:/mnt/c/Users/evams/Documents/OLC1_Proyecto2_202201989/server/node_modules/.pnpm/jison@0.4.18/node_modules/jison/node_modules:/mnt/c/Users/evams/Documents/OLC1_Proyecto2_202201989/server/node_modules/.pnpm/jison@0.4.18/node_modules:/mnt/c/Users/evams/Documents/OLC1_Proyecto2_202201989/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jison/lib/cli.js" "$@"
else
  exec node  "$basedir/../jison/lib/cli.js" "$@"
fi
